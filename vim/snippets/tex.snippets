		\\caption{${6:Caption}}
		\\label{figure:${7:Label}}
	\\end{figure}
endsnippet

snippet table
	\\begin{table}[h]
		%\\centering
		%\\onehalfspacing
		\\begin{tabularx}{\\linewidth}{${1:X l c r}}
			\\caption{${2:Caption}}
			\\toprule
			${3:Headers}
			\\midrule
			${4:Data}
			\\bottomrule
			\\label{table:${5:Label}}
		\\end{tabularx}
		\\caption{${6:Caption}}
	\\end{table}
endsnippet

snippet longtable
	\\begin{longtable}{${1:X l c r}}
		% First header
		\\caption{${2:Caption}}%
		\\label{table:${3:Label}}
		\\toprule
		${4:Headings} %Column headings
		\\midrule
		\\endfirsthead
		% Consequent headers
		\\caption{Continued from previous page}\\\\
		\\toprule
		$4 %Column headings
		\\midrule
		\\endhead
		% Footers of unfinished table
			Continued on next page\\\\
		\\endfoot
		% Last footer
		\\bottomrule
		\\endlastfoot
		%Data
		${5:Data} 
	\\end{longtable}
endsnippet

snippet ltxtable
	\\LTXtable{\\textwidth}{tables/${1:File}.tex}
endsnippet

snippet plot
	\\begin{figure}[h]
	\\input{${1:File}}
	%\\vspace*{1cm}
	\\caption{${2:Caption}}
	\\label{plot:${3:Label}}
	\\end{figure}
endsnippet

snippet blankpage "Inserts a blank page"
	\afterpage{\null\newpage}
endsnippet
